/* BUTTONS */
.button {
	text-indent: -9000px;  
	height: 25px;
	line-height:25px;
	/*\*/
	display: block;
	/*/
	display: inline-block;
 	/**/	
	text-decoration: none;
	cursor: pointer;
	padding: 0;
	margin: 0 5px 0 0;
	-moz-outline:none;
	background-color:transparent;
	border:0;
	float:left; 
}

.button:focus::-moz-focus-inner, .tab:focus::-moz-focus-inner { 
	/* removes dotted outline in FF */
	-moz-outline: none; 
	outline:none;
	border:none;
}
button::-moz-focus-inner { 
	/* removes dotted outline in FF */
	-moz-outline: none; 
	border: none;
	outline:none;
}

